#include <bits/stdc++.h>
using namespace std;
int main() 
{
int n;
scanf("%d",&n);
while(n--)
{
	string s;
	cin>>s;
	int i=0;
	int c=0;
	int l=s.length();
	int j=l-1;
	vector<int>v;
	while(i<l)
	{
		int flag=0;
		j=l-1;
		while(j>=0)
		{
		if(s[i]==s[j]&&i!=j)
		{
		c+=2;
		i++;
		}
		else
		c=0;
		if(i==j)
		c++;
		if(i>=j)
		{
		flag=1;
		break;
		}
		j--;
		}
		if(flag==1)
		break;
		cout<<"hi"<<endl;
		v.push_back(c);
	}
	cout<<c<<endl;
	if(c==l)
	cout<<"yES"<<endl;
	else
	cout<<"no"<<endl;
}
	return 0;
}
